# Configuración de la base de datos MySQL
DB_USER=          # Usuario de la base de datos
DB_PASSWORD=      # Contraseña del usuario
DB_HOST=          # Host de la base de datos (ej. localhost)
DB_PORT=          # Puerto de conexión (ej. 3306)
DB_NAME=          # Nombre de la base de datos

# Configuración de Redis
REDIS_HOST=       # Host de Redis (ej. localhost)
REDIS_PORT=       # Puerto de Redis (ej. 6379)
REDIS_PASSWORD=   # Contraseña de Redis (opcional)
CACHE_TTL=        # Tiempo de vida del caché en segundos (ej. 3600)